  "module": "index.ts",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "type": "commonjs",
  "license": "MIT",
  "version": "1.0.3",
  "repository": {
    "url": "git+https://github.com/jskong1124/tabby-charset-euckr.git"
  },
  "author": {
    "name": "jskong1124"
  },
  "keywords": [
    "tabby-plugin",
    "euc-kr",
    "korean",
    "charset",
    "encoding"
  ],
  "files": [
