    "apply-loader": "^2",
    "css-loader": "^6",
    "iconv-lite": "^0.6.3",
    "ngx-toastr": "^16",
    "sass": "^1.90.0",
    "sass-loader": "^13",
    "style-loader": "^3",
    "tabby-core": "latest",
    "tabby-electron": "latest",
    "tabby-settings": "latest",
    "tabby-terminal": "latest",
    "ts-loader": "^9",
    "typescript": "^4",
    "utils-decorators": "^2.0.7",
    "webpack": "^5",
    "webpack-cli": "^5"
  }
